import java.util.*;
public class Main
{
    public static void main (String[] args)
    {
        int arr[]={10,2,5,20,17};
        int min=arr[0];
        int max=arr[0];
        for(int i=0;i<arr.length;i++){
            if(arr[i]<min){
                min=arr[i];
            }
            if(arr[i]>max){
                max=arr[i];
            }
        }
        System.out.print("minimum"+min+"maximum"+max);
    }
}
