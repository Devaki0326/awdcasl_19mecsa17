import java.io.*;
import java.util.*;
class Replaceno
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int i=sc.nextInt();
int a=sc.nextInt();
int b=sc.nextInt();
int x=0,y=0,z=1;
while(i!=0)
{
x=i%10;
if(x==a)
{y=y+b*z;}
else
{y=y+x*z;}
z=z*10;
i=i/10;
}
System.out.println(y);
}}
